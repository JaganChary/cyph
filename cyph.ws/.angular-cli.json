{
	"$schema": "../node_modules/@angular/cli/lib/config/schema.json",
	"project": {
		"name": "cyph.ws"
	},
	"apps": [
		{
			"root": "src",
			"outDir": "dist",
			"assets": [
				"assets",
				"favicon.ico",
				"manifest.json",
				"serviceworker.js"
			],
			"index": "index.html",
			"main": "js/cyph.ws/main.ts",
			"polyfills": "js/cyph.ws/polyfills.ts",
			"test": "test.ts",
			"tsconfig": "tsconfig.app.json",
			"testTsconfig": "tsconfig.spec.json",
			"prefix": "cyph",
			"styles": [
				"css/cyph.ws.scss"
			],
			"scripts": [],
			"environmentSource": "js/environments/environment.ts",
			"environments": {
				"backup": "js/environments/environment.backup.ts",
				"beta": "js/environments/environment.beta.ts",
				"dev": "js/environments/environment.dev.ts",
				"e2e": "js/environments/environment.e2e.ts",
				"local": "js/environments/environment.local.ts",
				"localBackup": "js/environments/environment.local-backup.ts",
				"localBeta": "js/environments/environment.local-beta.ts",
				"localDev": "js/environments/environment.local-dev.ts",
				"localMaster": "js/environments/environment.local-master.ts",
				"localStaging": "js/environments/environment.local-staging.ts",
				"master": "js/environments/environment.master.ts",
				"prod": "js/environments/environment.prod.ts",
				"staging": "js/environments/environment.staging.ts",
				"tmp": "js/environments/.environment.tmp.ts"
			}
		}
	],
	"e2e": {
		"protractor": {
			"config": "./protractor.conf.js"
		}
	},
	"lint": [
		{
			"project": "src/tsconfig.app.json"
		},
		{
			"project": "src/tsconfig.spec.json"
		},
		{
			"project": "e2e/tsconfig.e2e.json"
		}
	],
	"test": {
		"karma": {
			"config": "./karma.conf.js"
		}
	},
	"defaults": {
		"styleExt": "scss",
		"component": {}
	}
}
